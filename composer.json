{
    "name": "genealabs/laravel-multi-step-progressbar",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0",
        "illuminate/view": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^8.5",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelMultiStepProgressbar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelMultiStepProgressbar\\Providers\\Service"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
